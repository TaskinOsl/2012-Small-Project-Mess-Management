}
<style type="text/css"> 
    body {
            background-image: url('../../images/image.jpg');        
            background-repeat:repeat-x;   
            background-position:top;
            height: inherit;
            background-size: cover;
            
        }</style>
  
<div id="full" style="width: 100%;height: 100%;padding: 0px 0px 0px 6%; ">
    <h2 style="color:white"> Details Information For [@Html.DisplayFor(model => model.FName)] </h2>
